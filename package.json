{
  "name": "full-stack-project",
  "version": "0.1.0",
  "private": true,
  "prettier": "prettier-airbnb-config",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "build": "npm run build -w frontend",
    "start": "npm run migrate && npm start -w backend",
    "migrate": "npm run migrate -w backend",
    "rollback": "npm run rollback -w backend",
    "dev": "concurrently \"npm run dev -w backend\" \"npm start -w frontend\"",
    "lint": "npm run lint -ws",
    "test": "npm run test -ws",
    "test:ci": "npm run test:ci -ws"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "prettier-airbnb-config": "^1.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}
